{
  "name": "hello-yb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel data:watch dev:next",
    "dev:next": "next dev --turbopack",
    "build": "npm-run-all data:build next:build",
    "next:build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "data:build": "tsx scripts/process-data.ts",
    "data:watch": "tsx scripts/process-data.ts --watch"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "chokidar": "^4.0.3",
    "csv-parse": "^6.1.0",
    "fs-extra": "^11.3.2",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.17.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
